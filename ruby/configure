	platform=`sed '/^option  *platform  */!d;s///;s/|/*|/g;q' "$file"`
	if test "x$platform" != x; then :

	    eval "case \"\$target_os\" in #(
  $platform*) :
    break ;; #(
  *) :
     ;;
esac"

fi
	setup=
    done
    : ${setup:=Setup}

fi


