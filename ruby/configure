	case "$rbcv_sizeof_type" in #(
  [0-9]*|SIZEOF_*) :

	    ${rbcv_sizeof_cond+echo "#else"}
	    echo "#define SIZEOF_CLOCK_T $rbcv_sizeof_type"
	    break
	     ;; #(
  *) :

