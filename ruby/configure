  if test "$cross_compiling" = yes; then :
  case $target_os in #(
  linux*|darwin*|*bsd|cygwin*|mingw*|mswin*) :
    rb_cv_getcwd_malloc=yes ;; #(
  *) :
    rb_cv_getcwd_malloc=no ;;
esac
